    "husky": "^8.0.2",
    "lint-staged": "^13.0.4",
    "prettier": "^2.8.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "yarn eslint --fix",
      "yarn prettier --write",
      "bash -c tsc --noEmit"
    ],
    "*.{md,json}": [
      "yarn prettier --write"
    ],
    "*.{css,scss,sass}": [
      "yarn lint:css --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
